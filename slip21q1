# include<unistd.h>
#include<stdio.h>
#include<sys/types.h>
void ChildProcess();
void ParentProcess();
int main()
{
pid_t pid;
pid=fork();
if(pid==0)
ChildProcess();
else
ParentProcess();
return 0;
}
void ChildProcess()
{
printf("i am child process..\n");
printf("the process id of child..%d\n",getpid());
}
void ParentProcess()
{
printf("i am parent process..\n");
printf("the process id of parent..%d\n",getppid());
}
